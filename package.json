{
  "name": "octi-realtime-backend",
  "version": "1.0.0",
  "description": "Backend WebSocket pour agent IA vocal OCTI utilisant OpenAI Realtime API",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "type-check": "tsc --noEmit",
    "ingest": "ts-node scripts/ingest.ts"
  },
  "keywords": [
    "openai",
    "realtime",
    "websocket",
    "voice-assistant"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@pinecone-database/pinecone": "^3.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "openai": "^4.104.0",
    "pdf-parse": "^1.1.4",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "ws": "^8.14.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pdf-parse": "^1.1.5",
    "@types/ws": "^8.5.10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
